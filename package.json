{
  "name": "moskool",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "@sentry/browser": "^5.12.1",
    "firebase": "^7.8.2",
    "material-table": "^1.57.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dom-confetti": "^0.1.3",
    "react-live": "^2.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-typist": "^2.0.5",
    "recompose": "^0.30.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "npm run lintAndFix && react-scripts build && firebase deploy --only hosting:tool-builder",
    "check-updates": "ncu -u",
    "test": "react-scripts test",
    "ci-test": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "lintAndFix": "eslint './src/**/*.js' --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lintAndFix",
      "pre-push": "npm run lintAndFix && npm ci-test"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0"
  }
}